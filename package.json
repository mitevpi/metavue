{
  "name": "js-module-template",
  "version": "0.0.2",
  "description": "My JS/NodeJS module",
  "license": "MIT",
  "repository": "mitevpi/js-module-template",
  "main": "dist/index.js",
  "author": {
    "name": "Petr Mitev",
    "email": "p.mitevpi@gmail.com",
    "url": "https://mitevpi.com/"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "samples"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint .",
    "docs": "documentation build src/index.js -f md -o docs/README.md && documentation build src/** -f html -o docs",
    "postdocs": "",
    "clean": "rimraf dist",
    "prebuild": "npm run docs && npm run clean",
    "build": "babel src -d dist",
    "prerelease": "npm run lint && npm test && npm run build",
    "release": "standard-version",
    "postpublish": "git push origin master --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "verbose": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "node",
    "module",
    "package",
    "template",
    "npm"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-flow": "7.7.4",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "documentation": "12.1.4",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "open-cli": "5.0.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "standard-version": "7.0.1"
  }
}
