{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": ["jest"],
  "env": {
    "jest": true,
    "jest/globals": true
  },
  "rules": {
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "max-classes-per-file": "off",
    "no-console": "off",
    "semi": ["error", "always"],
    "linebreak-style": "off"
  }
}
